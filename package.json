{
    "name": "web-free",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@craco/craco": "^6.4.5",
        "@faker-js/faker": "^7.6.0",
        "@reduxjs/toolkit": "^1.9.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^29.2.2",
        "@types/node": "^18.11.9",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.8",
        "axios": "^1.1.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.4.3",
        "react-scripts": "5.0.1",
        "typescript": "^4.8.4",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "craco start",
        "build": "craco build",
        "test": "craco test",
        "tsc": "tsc --noEmit",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@mswjs/data": "^0.11.0",
        "@trivago/prettier-plugin-sort-imports": "^3.4.0",
        "@tsconfig/create-react-app": "^1.0.3",
        "husky": "^8.0.0",
        "lint-staged": "^13.0.3",
        "msw": "^0.47.4",
        "prettier": "^2.7.1"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "jest": {
        "transformIgnorePatterns": [
            "node_modules/(?!axios)"
        ],
        "moduleNameMapper": {
            "^@components(.*)$": "<rootDir>/src/components$1",
            "^@features(.*)$": "<rootDir>/src/features$1"
        }
    }
}
